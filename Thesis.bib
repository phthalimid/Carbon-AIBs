	pages = {134806},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\M35FU647\\Elterman et al. - 2019 - Development of a novel 1-trifluoroacetyl piperidin.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\LBJLV5XF\\S0013468619316779.html:text/html}
