	file = {Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\FYJBUTD9\\Hsu and Lu - 2005 - Vapor−Solid Growth of Sn Nanowires  Growth Mechan.pdf:application/pdf}
}

