	file = {PubMed Central Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\BEU7KKLA\\Sharma et al. - 2018 - Stable and scalable 1T MoS2 with low temperature-c.pdf:application/pdf}
}

