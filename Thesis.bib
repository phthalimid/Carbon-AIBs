@article{gyenes_understanding_2015,
	title = {Understanding {Anomalous} {Behavior} in {Coulombic} {Efficiency} {Measurements} on {Li}-{Ion} {Batteries}},
	volume = {162},
	issn = {0013-4651, 1945-7111},
	url = {http://jes.ecsdl.org/content/162/3/A278},
	doi = {10.1149/2.0191503jes},
	abstract = {When identical dry Li-ion batteries are filled with different electrolyte solutions, those with higher coulombic efficiency (CE) normally have a longer life-time, provided the batteries have the same test history (storage time and temperature, cycling protocol etc.). Accurate CE measurements can therefore be used to rapidly rank cells according to their life-time without relying on tests that are many years long. The CE of Li-ion cells normally increases with time since SEI layers thicken and parasitic reactions between electrodes and electrolyte, which cause the departure of the CE from unity, slow down. However, in some cases, the CE of Li-ion cells has been observed to be greater than unity and decrease during early charge-discharge cycling, which is unexpected. This anomalous behavior is shown here to be caused by the impact of lithium atoms stored in the negative electrode “overhang”, the small portion of the negative electrode that extends past the positive electrode in commercial Li-ion cells. Storage protocols are discussed that allow CE to more rapidly stabilize with time or cycle number.},
	language = {en},
	number = {3},
	urldate = {2018-10-03},
	journal = {J. Electrochem. Soc.},
	author = {Gyenes, Balazs and Stevens, D. A. and Chevrier, V. L. and Dahn, J. R.},
	month = jan,
	year = {2015},
