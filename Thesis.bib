	pages = {937--950},
	annote = {Ref. [EF]},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\IH5ETB77\\Frackowiak and Béguin - 2001 - Carbon materials for the electrochemical storage o.pdf:application/pdf;ScienceDirect Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\GEB3SCSL\\Frackowiak and Béguin - 2001 - Carbon materials for the electrochemical storage o.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\WXFFCBDN\\S0008622300001834.html:text/html;ScienceDirect Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\RWS8HVA6\\S0008622300001834.html:text/html}
