	file = {Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\PIHYEG5L\\Zhang et al. - 2018 - Porous CuO microsphere architectures as high-perfo.pdf:application/pdf;Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\G2ZLFKKA\\C7TA10632G.html:text/html}
}

