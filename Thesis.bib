	month = mar,
	year = {2013},
	note = {tex.ids: wu\_functionalized\_2013-1},
	pages = {6055--6059},
	file = {ACS Full Text PDF w/ Links:C\:\\Users\\divyash\\Zotero\\storage\\ET6GXJJ9\\Wu et al. - 2013 - Functionalized Graphitic Carbon Nitride for Effici.pdf:application/pdf;ACS Full Text Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\KGAWQRZ5\\jp311972f.html:text/html;Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\YLYSQJTD\\Wu et al. - 2013 - Functionalized Graphitic Carbon Nitride for Effici.pdf:application/pdf}
}

