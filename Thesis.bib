	keywords = {Aluminum-ion battery, AlCl/EtNHCl ionic liquid electrolyte, graphene cathode, long cycle, Triethylamine hydrochloride},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\8DJM29X5\\Xu et al. - 2018 - Low-cost AlCl3Et3NHCl electrolyte for high-perfor.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\AUI8DT4A\\S2405829718306767.html:text/html}
}

@article{eftekhari_low_2017,
