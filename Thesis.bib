	note = {tex.ids: kang\_recent\_2017-1},
	pages = {7667--7690},
	file = {Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\A9NRNPM5\\Kang et al. - 2017 - Recent progress in layered metal dichalcogenide na.pdf:application/pdf;Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\T2P33AAJ\\Kang et al. - 2017 - Recent progress in layered metal dichalcogenide na.pdf:application/pdf;Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\29VZHQPJ\\c7ta00003k.html:text/html;Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\PIKZCRFI\\c7ta00003k.html:text/html}
