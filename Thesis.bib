	journal = {ChemNanoMat},
	author = {Xu, Cheng and Li, Jialin and Chen, Haixia and Zhang, Jianxin},
	keywords = {Electrolyte, Aluminum ion battery, Graphite, Benzyltriethylammonium chloride},
	file = {Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\K583PP26\\cnma.html:text/html}
}

