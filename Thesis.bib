	doi = {10.1016/j.ensm.2019.03.001},
	abstract = {Rechargeable aluminum ion batteries (AIBs) with low cost and nonflammability have attracted considerable interest for electronics and grid energy storage, however, developing densely-compact cathodes, with rapid ion/electron transport channels and high energy storage capability remains challenging. Herein, we reported the facile construction of the nanoporous densely-stacked films derived from three-dimensional (3D) graphene aerogels, prepared by the self-propagating combustion rapid reduction of graphene oxide aerogels within seconds, as advanced binder-free cathode for ultrafast and high-capacity AIBs. Owing to the notable characteristics of 3D interconnected yet nanoporous structure, large surface area (513 m2 g−1), high electrical conductivity (581 S cm−1), dense stacking (0.61 g cm−3), expanded interlayer spacing (3.69 Å) of graphene aerogel-derived compact film, the as-assembled AIBs deliver considerably high capacity of 245 mAh g−1 at 1 A g−1, at least twice of graphite for AIBs. Impressively, our AIBs display exceptional rate capability, showing 70 mAh g−1 at a high current density of 15 A g−1, coupled with superstable cycling stability without obvious capacity decay after 5000 times, and wide temperature operation from 0 to 60 °C, outperforming most reported carbon based cathodes for AIBs.},
	urldate = {2019-03-17},
	journal = {Energy Storage Materials},
	author = {Huang, Haibo and Zhou, Feng and Shi, Xiaoyu and Qin, Jieqiong and Zhang, Zhigang and Bao, Xinhe and Wu, Zhong-Shuai},
	month = mar,
	year = {2019},
	keywords = {Aluminum ion batteries, Graphene, Aerogels, High capacity, Rapid reduction},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\ZVHGGHRC\\Huang et al. - 2019 - Graphene aerogel derived compact films for ultrafa.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\C9HM8IWI\\S2405829718311814.html:text/html}
}

