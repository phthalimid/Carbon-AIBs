	year = {2016},
	pages = {872--877},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\LYXZD8RP\\Pramanick et al. - 2016 - Human hair-derived hollow carbon microfibers for e.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\HLD55IVE\\S0008622316305504.html:text/html}
}

@article{satish_macroporous_2015,
