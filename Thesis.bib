	file = {ScienceDirect Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\ZYLNK5CU\\Flandrois and Simon - 1999 - Carbon materials for lithium-ion rechargeable batt.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\TXLB2TLW\\S0008622398002905.html:text/html}
}

@article{aurbach_correlation_1999,
	title = {On the correlation between surface chemistry and performance of graphite negative electrodes for {Li} ion batteries},
	volume = {45},
	issn = {0013-4686},
	url = {http://www.sciencedirect.com/science/article/pii/S0013468699001942},
	doi = {10.1016/S0013-4686(99)00194-2},
	abstract = {This paper discusses some important aspects of the correlation between surface chemistry, 3D structure, and the electrochemical behavior of lithiated graphite electrodes. By reviewing results obtained with different electrolyte solutions (e.g. ethylene carbonate-based solutions, propylene carbonate solutions, and ether-based systems), we describe the stabilization and capacity fading mechanisms of graphite electrodes. One of the failure mechanisms occurs at potentials {\textgreater}0.5 V Li/Li+, and relates to an increase in the electrode's impedance due to improper passivation and a simultaneous change in the electrode's morphology, probably due to gas formation. At low potentials (depending on the electrolyte solution involved), phenomena such as exfoliation and amorphization of the graphite electrodes can be observed. Stabilization mechanisms are also discussed. In general, surface stabilization of the graphite is essential for obtaining reversible lithiation and a long electrode cycle life. The latter usually relates to precipitation of highly compact and insoluble surface species, which adhere well, and irreversibly, to the active surface. Hence, the choice of appropriate electrolyte solutions in terms of solvents, salts and additives is very critical for the use of graphite anodes in Li batteries. The major analytical tools for this study included FTIR and impedance spectroscopies, XPS, and in situ and ex situ XRD in conjunction with standard electrochemical techniques.},
	language = {en},
	number = {1},
	urldate = {2019-11-07},
	journal = {Electrochimica Acta},
	author = {Aurbach, D and Markovsky, B and Weissman, I and Levi, E and Ein-Eli, Y},
	month = sep,
	year = {1999},
	keywords = {FTIR, XPS, EDAX, EIS, In situ XRD, Li metal and Li ion batteries, Lithiated graphite, Lithium electrodes, Surface chemistry},
	pages = {67--86},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\3SZBPL4T\\Aurbach et al. - 1999 - On the correlation between surface chemistry and p.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\69YGY3C7\\S0013468699001942.html:text/html}
}

@article{aurbach_short_2002,
