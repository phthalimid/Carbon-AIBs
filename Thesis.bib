	author = {Zhang, Leyuan and Chen, Liang and Luo, Hao and Zhou, Xufeng and Liu, Zhaoping},
	month = aug,
	year = {2017},
	keywords = {graphene, aluminum-ion batteries, intercalation chemistry, size effect},
