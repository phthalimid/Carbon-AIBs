	keywords = {Battery fault diagnosis, Battery management system, Battery states estimate, Battery uniformity and equalization, Cell voltage measurement, Vehicle lithium-ion battery in electric vehicles},
	pages = {272--288},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\ZDRAAD44\\Lu et al. - 2013 - A review on the key issues for lithium-ion battery.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\TYN65CX4\\S0378775312016163.html:text/html}
}

