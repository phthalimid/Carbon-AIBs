@article{chai_optimized_2018,
	title = {Optimized expanding of interlayer distance for molybdenum disulfide towards enhanced hydrogen evolution reaction},
	volume = {428},
	issn = {0169-4332},
	url = {http://www.sciencedirect.com/science/article/pii/S0169433217328775},
	doi = {10.1016/j.apsusc.2017.09.229},
	abstract = {Adjusting the hydrogen-binding free energy (ΔGH*) of two dimensional (2D) MoS2 by changing the interlayer distance has been an effective strategy to improve the intrinsic activity for hydrogen evolution reaction (HER). Herein, a facile solvothermal access via various ratio of N, N-dimethylformamide (DMF)/H2O has been used to modulate interlayer distance of nanostructured MoS2. With increasing of DMF, the interlayer distance of MoS2 can be expanded to 9.4Å with smaller sizes, which may be derived from intercalation effect of DMF. The certain ratio of DMF/H2O (volume ratio of 19/1, MoS2-D19H1) leads to the largest interlayer distances of 10.0Å and the smallest size of nanospheres with less stacking than counterparts synthesized at other ratios of DMF/H2O. The expanded interlayer distance of MoS2-D19H1 may change electronic structure of active sites for HER, implying the improved ΔGH* and the intrinsic activity of MoS2. The smallest size also suggests the more exposure of active sites for HER. The electrochemical measurements demonstrate that MoS2-D19H1 shows the best electrocatalytic performances than MoS2 samples synthesized at other ratio of DMF/H2O. This work may provide a promising strategy to tune the interlayer distance of 2D-layered transition metal dichalcogenide for efficient HER.},
	language = {en},
	urldate = {2019-10-23},
	journal = {Applied Surface Science},
	author = {Chai, Yong-Ming and Shang, Xiao and Hu, Wen-Hui and Dong, Bin and Liu, Zi-Zhang and Chi, Jing-Qi and Yan, Kai-Li and Gao, Wen-Kun and Liu, Chen-Guang},
	month = jan,
	year = {2018},
	keywords = {MoS, Interlayer spacing, Hydrogen evolution reaction, N, N-dimethylformamide, Solvothermal},
	pages = {948--953},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\FFB4DASN\\Chai et al. - 2018 - Optimized expanding of interlayer distance for mol.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\QXKDUDQA\\S0169433217328775.html:text/html}
}

@article{truss_quantitative_2016,
