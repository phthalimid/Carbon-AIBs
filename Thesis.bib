	note = {tex.ids: myung\_electrochemical\_2011-1},
	pages = {9891--9911},
	file = {Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\Z3EVPCAK\\Myung et al. - 2011 - Electrochemical behavior and passivation of curren.pdf:application/pdf;Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\TCFG73RX\\Myung et al. - 2011 - Electrochemical behavior and passivation of curren.pdf:application/pdf;Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\9MV3VHHN\\c0jm04353b.html:text/html;Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\TVWEQG5B\\c0jm04353b.html:text/html}
