	month = aug,
	year = {2016},
	keywords = {electrochemistry, ionic liquids, aluminum/sulfur batteries, solid-state reactions, sulfur},
	pages = {9898--9901},
	file = {Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\HTJPQ8WA\\Gao et al. - 2016 - A Rechargeable AlS Battery with an Ionic-Liquid E.pdf:application/pdf;Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\7AHMSH3V\\abstract\;jsessionid=F34490EAF33A180134513189DD096273.html:text/html}
