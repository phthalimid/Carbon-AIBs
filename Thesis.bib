	pages = {149--157},
	file = {Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\SYYWDLZE\\Raidongia Kalyan et al. - 2009 - BCN A Graphene Analogue with Remarkable Adsorptiv.pdf:application/pdf;Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\6DGQYAK7\\chem.html:text/html}
