	pages = {53--59},
	annote = {Ref. [BM]},
	file = {ScienceDirect Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\PLNDL9QT\\S0378775397024683.html:text/html}
}

@article{eliad_ion_2001,
