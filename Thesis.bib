	keywords = {aluminum, Energy density, aluminum chloride−graphite battery, flake, kish graphite},
	pages = {28478--28485},
	file = {ACS Full Text PDF w/ Links:C\:\\Users\\divyash\\Zotero\\storage\\VIMWWVCE\\Wang et al. - 2017 - Kish Graphite Flakes as a Cathode Material for an .pdf:application/pdf;ACS Full Text Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\JMILNR4I\\acsami.html:text/html}
}

