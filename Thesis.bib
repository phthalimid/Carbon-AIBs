	note = {Citation Key Alias: liu\_revitalizing\_2017-1, liu\_revitalizing\_2017-2},
	pages = {17705--17733},
	file = {Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\SDN89HYE\\Liu et al. - 2017 - Revitalizing carbon supercapacitor electrodes with.pdf:application/pdf;Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\FA26K9HC\\c7ta05646j.html:text/html}
}

@article{long_voltammetric_1999,
