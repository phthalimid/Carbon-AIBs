	keywords = {Lithium-ion batteries, Graphene sheets, Graphite oxide, Thermal exfoliation},
	pages = {3909--3914},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\MRKI5AQ3\\Lian et al. - 2010 - Large reversible capacity of high quality graphene.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\K8TDFK8W\\S0013468610002598.html:text/html}
}

@article{liu_revitalizing_2017,
