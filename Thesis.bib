@article{pieta_mechanism_2010,
	title = {Mechanism of {Reductive} {C60} {Electropolymerization} in the {Presence} of {Dioxygen} and {Application} of the {Resulting} {Fullerene} {Polymer} for {Preparation} of a {Conducting} {Composite} with {Single}-{Wall} {Carbon} {Nanotubes}},
	volume = {114},
	issn = {1932-7447},
	url = {https://doi.org/10.1021/jp912033q},
	doi = {10.1021/jp912033q},
	abstract = {The superoxide anion radical, O2•−, induced C60 electropolymerization mechanism was refined by simultaneous cyclic voltammetric (CV) and vis-NIR spectroelectrochemical as well as mass spectrometric (MALDI-TOF) characterization of the one- and two-electron reduction products of C60 in the presence of O2 in a mixed organic solvent solution. The C60 polymer (C60-O) film was also investigated by Raman spectroscopy and imaged by atomic force microscopy (AFM) both at the early and advanced polymerization stage. While the spectroelectrochemical behavior of the C60/C60•− couple in the presence of O2 was similar to that in its absence, at more negative potentials corresponding to C602− and O2•− formation C602− participated in a chemical follow-up reaction resulting in a product lacking any diagnostic absorption band in the vis-NIR range. Although the main peak in the MS spectrum of the one-electron reduction product was that of C60 at m/z of 720, several additional peaks in the m/z range of 739−760 appeared, indicating generation of C60O•− and C60O2•− followed by their protonation. Interestingly, the MS spectrum of the product of two-electron reduction of C60 revealed several peaks in the m/z range of 1297−1465. These peaks correspond to the oxygen-containing fullerene protonated dimers, which lose the Cn (n = 1, 2) or C2n (n = 2−5) fragments upon ionization. Apparently, products of the C602− and O2•− interaction spontaneously dimerize in the electrode vicinity. Importantly, oxygen is built into the dimeric molecule in the initial stage of electropolymerization. The Raman spectroscopy measurements and AFM surface imaging of the C60-O film revealed that the first CV cycle resulted in an electrodeposition of dimers. With the increase of the number of CV cycles, the extent of polymerization increased and the polymer structure became highly heterogeneous. Finally, an electrophoretically deposited film of the HiPCO single-wall carbon nanotubes (pyr-SWCNTs) noncovalently surface modified with 1-pyrenebutyric acid was coated by electropolymerization with the C60-O film under CV conditions to result in a polymer−CNT composite material. AFM imaging of this film showed that tangles of the pyr-SWCNTs bundles, coated with the C60-O globules, were formed. The electrochemical and viscoelastic properties of the pyr-SWCNTs{\textbar}C60-O film were unraveled by simultaneously performed CV and piezoelectric microgravimetry (PM) measurements in a blank (TBA)ClO4 acetonitrile solution. Specific capacitance of the electrode coated with this composite film was 184 F g−1, a value comparable to those for other SWNT composite film coated electrodes, suggesting a plausible application of this material in developing supercapacitors.},
	number = {18},
	urldate = {2019-03-15},
	journal = {J. Phys. Chem. C},
	author = {Pieta, Piotr and Zukowska, Grazyna Z. and Das, Sushanta K. and D’Souza, Francis and Petr, Andreas and Dunsch, Lothar and Kutner, Wlodzimierz},
	month = may,
	year = {2010},
	note = {tex.ids: pieta\_mechanism\_2010-1},
	pages = {8150--8160},
	file = {ACS Full Text PDF w/ Links:C\:\\Users\\divyash\\Zotero\\storage\\DHS6QJMM\\Pieta et al. - 2010 - Mechanism of Reductive C60 Electropolymerization i.pdf:application/pdf;ACS Full Text PDF w/ Links:C\:\\Users\\divyash\\Zotero\\storage\\97FBERWF\\Pieta et al. - 2010 - Mechanism of Reductive C60 Electropolymerization i.pdf:application/pdf;ACS Full Text Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\QIMQTJYI\\jp912033q.html:text/html;ACS Full Text Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\M9MVD9JV\\jp912033q.html:text/html}
}

@article{grodzka_electrochemical_2008,
	title = {Electrochemical formation and properties of two-component films of transition metal complexes and {C60} or {C70}},
