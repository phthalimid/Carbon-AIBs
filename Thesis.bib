	pages = {16114},
	file = {Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\TZWK98WZ\\Tikekar et al. - 2016 - Design principles for electrolytes and interfaces .pdf:application/pdf;Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\QTS69FZ3\\nenergy2016114.html:text/html}
}

