	file = {IOP Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\CUT58KSH\\Wang et al. - 2015 - Synthesis, properties and applications of 2D non-g.pdf:application/pdf;IOP Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\4V8H56IN\\Wang et al. - 2015 - Synthesis, properties and applications of 2D non-g.pdf:application/pdf}
