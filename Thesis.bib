	file = {ScienceDirect Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\ZGR73A5U\\Rahman et al. - 2019 - High temperature and high rate lithium-ion batteri.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\5TRHWRPB\\S2405829719301990.html:text/html}
