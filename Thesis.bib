	keywords = {Ionic liquid, Supercapacitors, Energy Storage, Molten Salts, Quasi-elastic neutron scattering},
	pages = {A507--A514},
	file = {Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\P7LJBN6K\\Osti et al. - 2019 - Cation Molecular Structure Affects Mobility and Tr.pdf:application/pdf;Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\5ZIWTWIJ\\A507.html:text/html}
