	file = {ACS Full Text Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\KPALBFFH\\acsami.html:text/html;Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\N5QSG9CU\\Heubner et al. - 2018 - Scalable Fabrication of Nanostructured Tin Oxide A.pdf:application/pdf}
}

