}

@article{pullanchiyodan_silver-decorated_2017,
	title = {Silver-{Decorated} {Boron} {Nitride} {Nanosheets} as an {Effective} {Hybrid} {Filler} in {PMMA} for {High}-{Thermal}-{Conductivity} {Electronic} {Substrates}},
	volume = {2},
	issn = {2470-1343},
	url = {https://doi.org/10.1021/acsomega.7b01436},
	doi = {10.1021/acsomega.7b01436},
	abstract = {High-thermal-conductivity and low-dielectric-loss polymer nanocomposites have gained tremendous attention in microelectronics technology. Against this background, the present study deals with the development of a high-thermal-conductivity, low-dielectric-constant, and low-loss polymer nanocomposite based on silver nanoparticle (AgNP)-decorated boron nitride nanosheets (BNNSs) as the filler in poly(methyl methacrylate) (PMMA) matrix. The nanocomposites were prepared through a facile solution-blending process. Elemental mapping of the prepared nanocomposite indicates the uniform distribution of filler particle in PMMA matrix. An impressive high-thermal conductivity (TC) enhancement of around 363\% was achieved for nanocomposite of 0.35 Vf of hybrid filler (1.48 W/m K) compared to pristine PMMA (0.32 W/m K). The addition of AgNP reduces the thermal contact resistance (Rc) by bridging individual BNNS, thereby improving thermal transport. Measured TC values were fitted with a theoretical model that showed good agreement. Dielectric measurements performed at radiofrequencies and microwave frequencies revealed that the nanocomposites show a low dielectric constant ({\textless}5), low loss (10–2), and very low alternating current conductivity (10–7 S/cm). The results suggest that silver-decorated BNNS is a promising hybrid filler for effective thermal management.},
	number = {12},
	urldate = {2018-03-20},
	journal = {ACS Omega},
	author = {Pullanchiyodan, Abhilash and S. Nair, Kanakangi and Surendran, Kuzhichalil P.},
	month = dec,
	year = {2017},
	pages = {8825--8835},
	file = {ACS Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\JGYJXBFX\\Pullanchiyodan et al. - 2017 - Silver-Decorated Boron Nitride Nanosheets as an Ef.pdf:application/pdf;ACS Full Text Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\L3HKB8VP\\acsomega.html:text/html}
}

@article{lei_boron_2015,
	title = {Boron nitride colloidal solutions, ultralight aerogels and freestanding membranes through one-step exfoliation and functionalization},
	volume = {6},
	copyright = {2015 Nature Publishing Group},
	issn = {2041-1723},
	url = {https://www.nature.com/articles/ncomms9849},
	doi = {10.1038/ncomms9849},
	abstract = {Manufacturing of aerogels and membranes from hexagonal boron nitride (h-BN) is much more difficult than from graphene or graphene oxides because of the poor dispersibility of h-BN in water, which limits its exfoliation and preparation of colloidal solutions. Here, a simple, one-step mechano-chemical process to exfoliate and functionalize h-BN into highly water-dispersible, few-layer h-BN containing amino groups is presented. The colloidal solutions of few-layer h-BN can have unprecedentedly high concentrations, up to 30 mg ml−1, and are stable for up to several months. They can be used to produce ultralight aerogels with a density of 1.4 mg cm−3, which is ∼1,500 times less than bulk h-BN, and freestanding membranes simply by cryodrying and filtration, respectively. The material shows strong blue light emission under ultraviolet excitation, in both dispersed and dry state.},
	language = {en},
	urldate = {2018-03-20},
	journal = {Nature Communications},
	author = {Lei, Weiwei and Mochalin, Vadym N. and Liu, Dan and Qin, Si and Gogotsi, Yury and Chen, Ying},
	month = nov,
	year = {2015},
	pages = {8849},
	file = {Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\2V4FPAU6\\Lei et al. - 2015 - Boron nitride colloidal solutions, ultralight aero.pdf:application/pdf;Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\2E6ZNCYZ\\ncomms9849.html:text/html}
}

