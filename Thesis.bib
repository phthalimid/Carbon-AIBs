	volume = {55},
	issn = {1521-3773},
	url = {http://onlinelibrary.wiley.com/doi/10.1002/anie.201605515/abstract},
	doi = {10.1002/anie.201605515},
	abstract = {The controlled exfoliation of hexagonal boron nitride (h-BN) into single- or few-layered nanosheets remains a grand challenge and becomes the bottleneck to essential studies and applications of h-BN. Here, we present an efficient strategy for the scalable synthesis of few-layered h-BN nanosheets (BNNS) using a novel gas exfoliation of bulk h-BN in liquid N2 (L-N2). The essence of this strategy lies in the combination of a high temperature triggered expansion of bulk h-BN and the cryogenic L-N2 gasification to exfoliate the h-BN. The produced BNNS after ten cycles (BNNS-10) consisted primarily of fewer than five atomic layers with a high mass yield of 16–20 \%. N2 sorption and desorption isotherms show that the BNNS-10 exhibited a much higher specific surface area of 278 m2 g−1 than that of bulk BN (10 m2 g−1). Through the investigation of the exfoliated intermediates combined with a theoretical calculation, we found that the huge temperature variation initiates the expansion and curling of the bulk h-BN. Subseqently, the L-N2 penetrates into the interlayers of h-BN along the curling edge, followed by an immediate drastic gasification of L-N2, further peeling off h-BN. This novel gas exfoliation of high surface area BNNS not only opens up potential opportunities for wide applications, but also can be extended to produce other layered materials in high yields.},
	language = {en},
	number = {36},
	urldate = {2018-03-12},
	journal = {Angew. Chem. Int. Ed.},
	author = {Zhu, Wenshuai and Gao, Xiang and Li, Qian and Li, Hongping and Chao, Yanhong and Li, Meijun and Mahurin, Shannon M. and Li, Huaming and Zhu, Huiyuan and Dai, Sheng},
	month = aug,
	year = {2016},
	keywords = {nanosheets, boron nitride, exfoliation, liquid nitrogen, thermal expansion},
	pages = {10766--10770},
	file = {Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\PNPNL9GX\\Zhu et al. - 2016 - Controlled Gas Exfoliation of Boron Nitride into F.pdf:application/pdf;Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\N784X7I7\\abstract.html:text/html}
}

@article{zheng_facile_2016,
	title = {Facile one-step and high-yield synthesis of few-layered and hierarchically porous boron nitride nanosheets},
