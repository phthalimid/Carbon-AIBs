	url = {http://www.sciencedirect.com/science/article/pii/S2211285517303920},
	doi = {10.1016/j.nanoen.2017.06.038},
	abstract = {Few layer graphene is a promising cathode material for aluminum-ion batteries that use chloroaluminate (AlCl4-) ionic liquids as the electrolyte. A fundamental understanding of interactions between the few layer graphene cathode and the ionic liquid electrolyte is key for realizing the full potential of these systems. Through in situ Raman spectroscopy and density functional theory calculations, we show that the cathode is capable of achieving stage-one intercalation within the operating voltage window, leading to improved cell performance. We also show that the presence of structural defects in few layer graphene such as pores induced via plasma exposure or nitrogen dopants can deteriorate the cell performance by either decreasing the electrical connectivity or precluding stage-one intercalation respectively. The cathodes made with highly crystalline few layer graphene display high power and energy densities ({\textasciitilde} 200Whkg−1 at 200Wkg−1 and {\textasciitilde} 160Whkg−1 at 5000Wkg−1), and are stable with no loss in performance up to 1000 cycles while fully charging to 2.4V.},
	urldate = {2018-03-07},
	journal = {Nano Energy},
	author = {Childress, Anthony S. and Parajuli, Prakash and Zhu, Jingyi and Podila, Ramakrishna and Rao, Apparao M.},
	month = sep,
	year = {2017},
	keywords = {Aluminum ion battery, Graphite intercalation compound, In situ Raman spectroscopy, Nitrogen doping, Graphene},
	pages = {69--76},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\67FT4H7I\\Childress et al. - 2017 - A Raman spectroscopic study of graphene cathodes i.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\Y2SMHQ6A\\S2211285517303920.html:text/html}
}

@article{li_design_2017,
	title = {Design principles and energy system scale analysis technologies of new lithium-ion and aluminum-ion batteries for sustainable energy electric vehicles},
	volume = {71},
	issn = {1364-0321},
	url = {http://www.sciencedirect.com/science/article/pii/S1364032116311510},
	doi = {10.1016/j.rser.2016.12.094},
	abstract = {Battery power is one of the most important sources of energy for vehicles that do not produce harmful gases, electric vehicles. These electric vehicles are also capable of taking advantage of the electric grid to recharge at night. Scientists worldwide are searching for practical battery designs and electrodes with high cycling stability for electric vehicles by combining nanotechnology with surface coating technologies. Multiple tests have been performed upon lithium-ion batteries; however, new research is focusing on aluminum-ion batteries. The production and application of this form of battery technology is expected to improve greatly in the future. This Review summarizes the recent highlights in the energy industry as well as our laboratory work regarding lithium-ion and aluminum-ion batteries. The focus of this work is on battery structure models and nanoscale analysis technologies. Furthermore, this Review outlines the challenges that exist in producing cheaper and more accessible batteries by examining the energy storage and transmission principles of these new batteries. The structure and size effects of nanoparticles allows, as well as probes on the thermodynamic mechanism for mediating lessened battery performance due to heat expansion of the nanostructure. Finally, this Review looks at batteries and electrodes of electric vehicles as objects, commenting on the design ideas and feasibility of new battery technologies.},
	urldate = {2018-03-07},
	journal = {Renewable and Sustainable Energy Reviews},
	author = {Li, Yong and Yang, Jie and Song, Jian},
	month = may,
	year = {2017},
	keywords = {Battery electrode characteristics, Design model, New energy system battery, Scale analysis technologies, Sustainable energy electric vehicles},
	pages = {645--651},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\6YZMHUXJ\\Li et al. - 2017 - Design principles and energy system scale analysis.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\HS5M9IXS\\S1364032116311510.html:text/html}
}

@article{wu_interface-reconstruction_2018,
	series = {Post {Lithium} ion batteries},
	title = {An interface-reconstruction effect for rechargeable aluminum battery in ionic liquid electrolyte to enhance cycling performances},
	volume = {3},
	issn = {2468-0257},
	url = {http://www.sciencedirect.com/science/article/pii/S2468025717301413},
	doi = {10.1016/j.gee.2017.10.002},
	abstract = {Aluminum (Al) metal has been regarded as a promising anode for rechargeable batteries because of its natural abundance and high theoretical specific capacity. However, rechargeable aluminum batteries (RABs) using Al metal as anode display poor cycling performances owing to interface problems between anode and electrolyte. The solid-electrolyte interphase (SEI) layer on the anode has been confirmed to be essential for improving cycling performances of rechargeable batteries. Therefore, we immerse the Al metal in ionic liquid electrolyte for some time before it is used as anode to remove the passive film and expose fresh Al to the electrolyte. Then the reactions of exposed Al, acid, oxygen and water in electrolyte are occurred to form an SEI layer in the cycle. Al/electrolyte/V2O5 full batteries with the thin, uniform and stable SEI layer on Al metal anode perform high discharge capacity and coulombic efficiency (CE). This work illustrates that an SEI layer is formed on Al metal anode in the cycle using a simple and effective pretreatment process and results in superior cycling performances for RABs.},
	number = {1},
	urldate = {2018-03-07},
	journal = {Green Energy \& Environment},
	author = {Wu, Feng and Zhu, Na and Bai, Ying and Gao, Yaning and Wu, Chuan},
	month = jan,
	year = {2018},
	keywords = {Aluminum anode, Rechargeable aluminum battery, Ionic liquid electrolyte, Solid-electrolyte interphase},
	pages = {71--77},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\divyash\\Zotero\\storage\\UEKAALRU\\Wu et al. - 2018 - An interface-reconstruction effect for rechargeabl.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\divyash\\Zotero\\storage\\T9TVN7BQ\\S2468025717301413.html:text/html}
}

