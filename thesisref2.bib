@article{largeot_relation_2008-2,
  title = {Relation between the {{Ion Size}} and {{Pore Size}} for an {{Electric Double}}-{{Layer Capacitor}}},
  volume = {130},
  issn = {0002-7863},
  abstract = {The research on electrochemical double layer capacitors (EDLC), also known as supercapacitors or ultracapacitors, is quickly expanding because their power delivery performance fills the gap between dielectric capacitors and traditional batteries. However, many fundamental questions, such as the relations between the pore size of carbon electrodes, ion size of the electrolyte, and the capacitance have not yet been fully answered. We show that the pore size leading to the maximum double-layer capacitance of a TiC-derived carbon electrode in a solvent-free ethyl-methylimmidazolium-bis(trifluoro-methane-sulfonyl)imide (EMI-TFSI) ionic liquid is roughly equal to the ion size ({$\sim$}0.7 nm). The capacitance values of TiC-CDC produced at 500 \textdegree{}C are more than 160 F/g and 85 F/cm3 at 60 \textdegree{}C, while standard activated carbons with larger pores and a broader pore size distribution present capacitance values lower than 100 F/g and 50 F/cm3 in ionic liquids. A significant drop in capacitance has been observed in pores that were larger or smaller than the ion size by just an angstrom, suggesting that the pore size must be tuned with sub-angstrom accuracy when selecting a carbon/ion couple. This work suggests a general approach to EDLC design leading to the maximum energy density, which has been now proved for both solvated organic salts and solvent-free liquid electrolytes.},
  number = {9},
  journal = {J. Am. Chem. Soc.},
  doi = {10.1021/ja7106178},
  author = {Largeot, Celine and Portet, Cristelle and Chmiola, John and Taberna, Pierre-Louis and Gogotsi, Yury and Simon, Patrice},
  month = mar,
  year = {2008},
  pages = {2730-2731},
  file = {C\:\\Users\\divyash\\Zotero\\storage\\NDPTIGGS\\Largeot et al. - 2008 - Relation between the Ion Size and Pore Size for an.pdf;C\:\\Users\\divyash\\Zotero\\storage\\3GN3BUHH\\ja7106178.html}
}

@article{samadi_group_2018,
