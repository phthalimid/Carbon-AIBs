  year = {2004},
  pages = {379-383},
  file = {C\:\\Users\\divyash\\Zotero\\storage\\9L2MJHAK\\Seki et al. - 2004 - Effect of binder polymer structures used in compos.pdf;C\:\\Users\\divyash\\Zotero\\storage\\A7Y3EBC2\\S0013468604006401.html}
}

