@article{guan_capacitive_2016,
  title = {Capacitive and Non-Capacitive Faradaic Charge Storage},
  volume = {206},
  issn = {0013-4686},
  abstract = {This article aims to offer a critical overview of selected literature on capacitive and non-capacitive faradaic charge storage. It is particularly relevant to the concept of pseudocapacitance that is generally described as a result of fast surface faradaic processes. In general, faradaic processes represent electron transfer reactions at the interface between an electrode and its contacting solid or liquid electrolyte phase that is able to accept or donate electrons. Obviously, not all faradaic processes can be associated with pseudocapacitance. The question is how to differentiate pseudocapacitance related faradaic charge storage from the others. Therefore, attempts have been made to apply the band model for semiconductors to account qualitatively for the origin of pseudocapacitance. Capacitive and non-capacitive faradaic processes are then proposed to define and differentiate different charge storage mechanisms in supercapacitor and battery. On the other hand, the unequal electrode capacitance approach and the use of Ca2+ in aqueous electrolytes are discussed in relation with enhanced energy capacity of supercapacitors. In addition, the principle of supercapattery as a hybrid device is explained with recent literature examples.},
  language = {en},
  journal = {Electrochimica Acta},
  doi = {10.1016/j.electacta.2016.01.213},
  author = {Guan, Li and Yu, Linpo and Chen, George Z.},
  month = jul,
  year = {2016},
  pages = {464-478},
  file = {C\:\\Users\\divyash\\Zotero\\storage\\4ZYH8TI6\\Guan et al. - 2016 - Capacitive and non-capacitive faradaic charge stor.pdf;C\:\\Users\\divyash\\Zotero\\storage\\4F7JQ6D5\\S0013468616302389.html}
}

@article{dupont_separating_2015,
  title = {Separating {{Faradaic}} and {{Non}}-{{Faradaic Charge Storage Contributions}} in {{Activated Carbon Electrochemical Capacitors Using Electrochemical Methods I}}. {{Step Potential Electrochemical Spectroscopy}}},
  volume = {162},
  issn = {0013-4651, 1945-7111},
  abstract = {Development of an electrochemical method for separating the contributions made by faradaic and non-faradaic processes to the total specific capacitance of an electrochemical capacitor electrode is reported. The method is based on step potential electrochemical spectroscopy (SPECS), and its application is demonstrated on activated carbon electrodes in aqueous media. The method involves applying a sequence of small potential steps ({$\pm$}25 mV) across the electrochemical window of the electrode material, with sufficient rest time between steps to allow for current equilibration. Modelling of the resultant i-t data using combined resistive, capacitive, diffusional and residual current terms allows for separation of the faradaic and non-faradaic terms. The behavior of these contributions across the electrode potential window is reported and discussed in comparison with conventional methods of electrochemical analysis, such as cyclic voltammetry.},
  language = {en},
  number = {7},
  journal = {J. Electrochem. Soc.},
  doi = {10.1149/2.0611507jes},
