  pages = {764-767},
  file = {C\:\\Users\\divyash\\Zotero\\storage\\MJNWYQN2\\Haigh et al. - 2012 - Cross-sectional imaging of individual layers and b.pdf;C\:\\Users\\divyash\\Zotero\\storage\\KDYSYWVF\\nmat3386.html}
}

