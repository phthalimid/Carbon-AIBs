  pages = {12555-12564},
  file = {C\:\\Users\\divyash\\Zotero\\storage\\2BIF3QWQ\\Fu et al. - 2014 - Covalently coupled hybrid of graphitic carbon nitr.pdf;C\:\\Users\\divyash\\Zotero\\storage\\2N65DTAQ\\C4NR03145H.html}
