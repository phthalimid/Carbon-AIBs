  journal = {J. Electrochem. Soc.},
  doi = {10.1149/2.0761507jes},
  author = {Younesi, Reza and Christiansen, Ane S{\ae}lland and Scipioni, Roberto and Ngo, Duc-The and Simonsen, S{\o}ren Bredmose and Edstr{\"o}m, Kristina and Hjelm, Johan and Norby, Poul},
