  file = {C\:\\Users\\divyash\\Zotero\\storage\\VHCJECNT\\Li and Li - 2011 - The effect of carbonyl, carboxyl and hydroxyl grou.pdf;C\:\\Users\\divyash\\Zotero\\storage\\S8RWJJ9C\\S1872580511600784.html}
}

