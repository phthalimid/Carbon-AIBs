  pages = {528-534},
  file = {C\:\\Users\\divyash\\Zotero\\storage\\WBWGKRK2\\Wang et al. - 2019 - Insights into dynamic molecular intercalation mech.pdf;C\:\\Users\\divyash\\Zotero\\storage\\NYSZSTND\\S0008622319300387.html}
