  file = {C\:\\Users\\divyash\\Zotero\\storage\\5VYEMZRR\\Zhu et al. - 2018 - Diethyl (thiophen-2-ylmethyl) phosphonate A Novel.pdf;C\:\\Users\\divyash\\Zotero\\storage\\F2YUU4DH\\c8ta01236a.html}
}

