@article{liu_comprehensive_2017,
  series = {Special {{Issue}} on {{Energy Storage}} and {{Conversion}}},
  title = {A Comprehensive Review on Recent Progress in Aluminum\textendash{}Air Batteries},
  volume = {2},
  issn = {2468-0257},
  abstract = {The aluminum\textendash{}air battery is considered to be an attractive candidate as a power source for electric vehicles (EVs) because of its high theoretical energy density (8100 Wh kg-1), which is significantly greater than that of the state-of-the-art lithium-ion batteries (LIBs). However, some technical and scientific problems preventing the large-scale development of Al\textendash{}air batteries have not yet to be resolved. In this review, we present the fundamentals, challenges and the recent advances in Al\textendash{}air battery technology from aluminum anode, air cathode and electrocatalysts to electrolytes and inhibitors. Firstly, the alloying of aluminum with transition metal elements is reviewed and shown to reduce the self-corrosion of Al and improve battery performance. Additionally for the cathode, extensive studies of electrocatalytic materials for oxygen reduction/evolution including Pt and Pt alloys, nonprecious metal catalysts, and carbonaceous materials at the air cathode are highlighted. Moreover, for the electrolyte, the application of aqueous and nonaqueous electrolytes in Al\textendash{}air batteries are discussed. Meanwhile, the addition of inhibitors to the electrolyte to enhance electrochemical performance is also explored. Finally, the challenges and future research directions are proposed for the further development of Al\textendash{}air batteries.},
  number = {3},
  journal = {Green Energy \& Environment},
  doi = {10.1016/j.gee.2017.06.006},
