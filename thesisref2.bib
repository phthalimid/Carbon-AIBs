@article{ponrouch_impact_2011,
  title = {On the Impact of the Slurry Mixing Procedure in the Electrochemical Performance of Composite Electrodes for {{Li}}-Ion Batteries: {{A}} Case Study for Mesocarbon Microbeads ({{MCMB}}) Graphite and {{Co3O4}}},
