@article{tu_molten_2019,
  title = {The Molten Chlorides for Aluminum-Graphite Rechargeable Batteries},
  issn = {0925-8388},
  abstract = {A low-cost, high-capacity Al-graphite rechargeable aluminum-ion battery is confirmed to be able to work stably in a quaternary AlCl3-NaCl-LiCl-KCl inorganic molten salt as electrolyte with the lowest eutectic temperature of less than 75~\textdegree{}C. The battery shows a stable specific capacity of 114.9~mAh g-1 at 200~mA~g-1 over 1500 cycles at 90~\textdegree{}C. Moreover, the capacity can remain at {$\sim$}85~mAh g-1 with a higher current density of 800~mA~g-1 over 2600 cycles at 90~\textdegree{}C. Importantly, even cycled at 80~\textdegree{}C and 100~\textdegree{}C, almost 100\% capacity retention can be achieved after 5800 cycles when the temperature is back to 90~\textdegree{}C, verifying the superior long-term cycling stability under various temperatures. The inherent low-cost and high capacity, as well as outstanding long-term cycling stability, makes it particularly favorable for large-scale energy storage devices.},
