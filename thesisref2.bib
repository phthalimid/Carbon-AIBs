  file = {C\:\\Users\\divyash\\Zotero\\storage\\PNPNL9GX\\Zhu et al. - 2016 - Controlled Gas Exfoliation of Boron Nitride into F.pdf;C\:\\Users\\divyash\\Zotero\\storage\\N784X7I7\\abstract.html}
