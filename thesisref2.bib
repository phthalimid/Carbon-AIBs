  pages = {90-204},
  file = {C\:\\Users\\divyash\\Zotero\\storage\\9GXF4EY9\\Samadi et al. - 2018 - Group 6 transition metal dichalcogenide nanomateri.pdf;C\:\\Users\\divyash\\Zotero\\storage\\JZX6ZERP\\c7nh00137a.html}
