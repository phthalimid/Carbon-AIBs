}

@article{stephenson_lithium_2013,
  title = {Lithium Ion Battery Applications of Molybdenum Disulfide ({{MoS2}}) Nanocomposites},
  volume = {7},
  issn = {1754-5706},
  abstract = {This is the first targeted review of the synthesis \textendash{} microstructure \textendash{} electrochemical performance relations of MoS2 \textendash{} based anodes and cathodes for secondary lithium ion batteries (LIBs). Molybdenum disulfide is a highly promising material for LIBs that compensates for its intermediate insertion voltage ({$\sim$}2 V vs. Li/Li+) with a high reversible capacity (up to 1290 mA h g-1) and an excellent rate capability (e.g. 554 mA h g-1 after 20 cycles at 50 C). Several themes emerge when surveying the scientific literature on the subject: first, we argue that there is excellent data to show that truly nanoscale structures, which often contain a nanodispersed carbon phase, consistently possess superior charge storage capacity and cycling performance. We provide several hypotheses regarding why the measured capacities in such architectures are well above the theoretical predictions of the known MoS2 intercalation and conversion reactions. Second, we highlight the growing microstructural and electrochemical evidence that the layered MoS2 structure does not survive past the initial lithiation cycle, and that subsequently the electrochemically active material is actually elemental sulfur. Third, we show that certain synthesis techniques are consistently demonstrated to be the most promising for battery applications, and describe these in detail. Fourth, we present our selection of synthesis methods that we believe to have a high potential for creating improved MoS2 LIB electrodes, but are yet to be tried.},
  language = {en},
  number = {1},
  journal = {Energy Environ. Sci.},
  doi = {10.1039/C3EE42591F},
  author = {Stephenson, Tyler and Li, Zhi and Olsen, Brian and Mitlin, David},
  month = dec,
  year = {2013},
  pages = {209-231},
  file = {C\:\\Users\\divyash\\Zotero\\storage\\5VRK8MP2\\Stephenson et al. - 2013 - Lithium ion battery applications of molybdenum dis.pdf;C\:\\Users\\divyash\\Zotero\\storage\\E669Z7L8\\c3ee42591f.html}
}

@article{kang_recent_2017,
  title = {Recent Progress in Layered Metal Dichalcogenide Nanostructures as Electrodes for High-Performance Sodium-Ion Batteries},
  volume = {5},
  issn = {2050-7496},
  abstract = {Sodium-ion batteries (SIBs) have emerged as a promising candidate for large-scale stationary energy storage in light of possible concerns over cost and abundance. Advanced sodium host materials are required for developing SIBs with high energy density, long cycling stability and high safety. Layered metal dichalcogenides (MX2, M = Mo, W, Sn, V, Ti; X = S, Se, Te) have become a hot spot for anode materials in SIBs due to their merits of high conductivity, mechanical and thermal stability and structural stability. In this review, we first present a comprehensive overview of the progress of layered MX2 anodes for SIBs. Detailed discussion on the advantages of MX2 as SIB anodes is then made. Emphasis is placed on enhancing the electrochemical performance through nanostructure engineering, crystal structure modulation, doping/alloying and composite design. We conclude with a perspective on the further development of SIBs in view of their applications.},
  language = {en},
  number = {17},
  journal = {J. Mater. Chem. A},
  doi = {10.1039/C7TA00003K},
