@article{elterman_development_2019,
  title = {Development of a Novel 1-Trifluoroacetyl Piperidine-Based Electrolyte for Aluminum Ion Battery},
  issn = {0013-4686},
  abstract = {The first example of chloroaluminate ionic liquids (ILs) analogues based on amides of fluorinated carboxylic acids, namely 1-trifluoroacetyl piperidine (TFAP), has been described. The possibility of applying of this electrolytes has been experimentally evaluated in the series TFAP/AlCl3; TFAP/LiCl/AlCl3; TFAP/[EMIm]Cl/AlCl3; [EMIm]Cl/AlCl3 for use in an aluminum ion battery. The specific conductivities measured by the method of current interruption and impedance spectroscopy at 30 \textdegree{}C were 0.49, 0.28, 1.37, 20.23 mS cm-1, respectively. The exchange current densities at the two-phase interface aluminium/electrolyte investigated using the impedance spectroscopy method were 0.18, 0.03, 0.42, 1100 {$\mu$}A cm-2, respectively. TFAP-based electrolytes did not crystallize when cooled down to -60 \textdegree{}C, according to DSC data. The presence of the molecular [AlCl3(TFAP)], and the ionic AlCl4-, [AlCl2(TFAP)4]+, and [Al(TFAP)6]3+species is proposed based on the 27Al NMR spectra of the studied new ILs analogues.},
  journal = {Electrochimica Acta},
  doi = {10.1016/j.electacta.2019.134806},
  author = {Elterman, V. A. and Shevelin, P. Yu and Chizhov, D. L. and Yolshina, L. A. and Il'ina, E. A. and Borozdin, A. V. and Kodess, M. I. and Ezhikova, M. A. and Rusinov, G. L.},
  month = sep,
