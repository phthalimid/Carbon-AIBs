  file = {C\:\\Users\\divyash\\Zotero\\storage\\4VP4EI6I\\Bhauriyal et al. - 2017 - The staging mechanism of AlCl4 intercalation in a .pdf;C\:\\Users\\divyash\\Zotero\\storage\\DDBSTGNM\\C7CP00453B.html}
